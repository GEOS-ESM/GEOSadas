#SBATCH --ntasks=%NCPUS_AOD%
#SBATCH --qos=%PRIORITY_AOD%
#SBATCH --time=%TIME_AOD%
#SBATCH --job-name=%STE_NAME%-aerosol-analysis
#SBATCH --output=%BATCHOUT_DIR%/%STE_NAME%-aerosol-analysis.out.%j

