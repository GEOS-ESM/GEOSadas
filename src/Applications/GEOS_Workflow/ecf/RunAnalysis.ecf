#!/usr/bin/env bash

%include <slurm.h>
#SBATCH --ntasks=%NCPUS_GSI%
#SBATCH --qos=%PRIORITY_GSI%
#SBATCH --time=%TIME_GSI%
#SBATCH --job-name=%STE_NAME%-run-analysis
#SBATCH --output=%BATCHOUT_DIR%/%STE_NAME%-run-analysis.out.%j

%include <head.h>

CWD=$(pwd)

export NCPUS_GSI=%NCPUS_GSI%
export NCPUS_SAC=%NCPUS_SAC%
export WORKFLOW_DIR=%ECF_HOME%
bash ${WORKFLOW_DIR}/tasks/RunAnalysis.sh

cd $CWD

%include <tail.h>
