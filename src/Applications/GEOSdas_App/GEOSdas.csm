# ------------------------------------
  Sub CopyGcmRestarts4Forecast_()
# ------------------------------------
  if ( $?ECHO___ ) set echo

  foreach rsf ( d $grs_list )   
     if ( "$rsf" == "d" ) then
        set rsfile = $fcstage/stage/$EXPID.rst.lcv.$itime.bin
     else
        set rsbase = $fcstage/stage/$EXPID.${rsf}_rst.$itime
        set rsfile = $rsbase.nc4
        if (! -e $rsfile) then
            set rsfile = $rsbase.bin
            if (! -e $rsfile) then
