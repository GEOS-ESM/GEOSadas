      /bin/mv $EXPID.*diag*ods     $EXPID.gsidiags.${nymdb}_${hh}z.iter$Viter_
      tar -cvf $EXPID.gsidiags.${nymdb}_${hh}z.iter$Viter_.tar $EXPID.gsidiags.${nymdb}_${hh}z.iter$Viter_
   endif

  endif

  exit 0

