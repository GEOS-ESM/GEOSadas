#     /bin/ln -s $fcstage/stage/$EXPID.ptrj_prs_rst.$itime_hhmm.$NCSUFFIX ./$EXPID.ptrj.prs.$itime_hhmm.$NCSUFFIX
# endif

  set dtg = ( `rst_date d_rst` )
