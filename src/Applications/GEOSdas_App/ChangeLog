/archive/u/rtodling/e512a_rt/chem/Y%y4/M%m2/e512a_ops.aod_a.sfc.%y4%m2%d2_%h200z.nc4 => g5nc_rt.aod_a.sfc.%y4%m2%d2_%h200z.nc4
/archive/u/rtodling/e512a_rt/chem/Y%y4/M%m2/e512a_ops.aod_f.sfc.%y4%m2%d2_%h200z.nc4 => g5nc_rt.aod_f.sfc.%y4%m2%d2_%h200z.nc4
/archive/u/rtodling/e512a_rt/chem/Y%y4/M%m2/e512a_ops.aod_k.sfc.%y4%m2%d2_%h200z.nc4 => g5nc_rt.aod_k.sfc.%y4%m2%d2_%h200z.nc4

