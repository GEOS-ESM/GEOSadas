NY: @NY

DYCORE: @DYCORE
DYN_GRIDNAME: @AGCM_GRIDNAME

DYN_IM: @AGCM_IM
DYN_JM: @AGCM_JM
DYN_LM: @AGCM_LM

DYN_DATE: >>>ANADATE<<<
DYN_TIME: >>>ANATIME<<<

AREA_FILENAME: ExtData/g5gcm/bcs/Ganymed-4_0/Shared/CF>>>AGCM_IM4<<<x6C.AREA.bin

#MAPL_ENABLE_BOOTSTRAP: NO
#SCLINC: 1.0

ALLOW_MOIST_AINC_UPDATE: 1

ALLOW_PCHEM_AINC_UPDATE: 1
